<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Locadora de Imóveis</title>
    <link rel="stylesheet" href="~/css/main.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Corretora_imovel_gustavo_martins.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Locadora</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Usuarios" asp-action="Index">Cadastro Usuário</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <!--Eu fazendo baianagem no teste-->
        <div id="page-wrapper">

        <!-- Header -->
        <div id="header-wrapper">
            <header id="header" class="container">

                <!-- Logo -->
                <div id="logo">
                    <h1><a href="home">Locadora</a></h1>
                    <span>by Gustavo Lopes</span>
                </div>

                <!-- Nav -->
                <nav id="nav">
                    <ul>
                        <li class="current"><a href="homesite.html">Casas</a></li>
                        <li><a href="homesite.html">Sobre mim</a></li>
                        <li><a href="painelusuario.aspx">Conteúdo</a></li>
                        <li><a href="login.aspx">Login Usuário</a></li>
                    </ul>
                </nav>

            </header>
        </div>

        <!-- Banner -->
        <div id="banner-wrapper">
            <div id="banner" class="box container">
                <div class="row">
                    <div class="col-7 col-12-medium">
                        <h2>Seja bem-vindo à Locadora.</h2>
                        <p>Escolha seu imóvel!</p>
                    </div>
                    <div class="col-5 col-12-medium">
                        <ul>
                            <li><a asp-area="" asp-controller="Home" class="button large icon solid fa-arrow-circle-right">Faça Login</a></li>
                            <li><a asp-area="" asp-controller="Home" class="button alt large icon solid fa-question-circle">Alugue seu imóvel</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>

        <!-- Features -->
        <div id="features-wrapper">
            <div class="container">
                <div class="row">
                    <div class="col-4 col-12-medium">

                        <!-- Box -->
                        <section class="box feature">
                            <a href="#" class="image featured"><img src="images/pic01.jpg" alt="" /></a>
                            <div class="inner">
                                <header>
                                    <h2>Imóvel N#1</h2>
                                    <p>Com jardim para criar cachorros e plantar uma hortinha</p>
                                </header>
                                <p>Olha a couuuuuve!.</p>
                            </div>
                        </section>

                    </div>
                    <div class="col-4 col-12-medium">

                        <!-- Box -->
                        <section class="box feature">
                            <a href="#" class="image featured"><img src="images/pic02.jpg" alt="" /></a>
                            <div class="inner">
                                <header>
                                    <h2>Imóvel N#2</h2>
                                    <p>Linda casa com piscina, confia!</p>
                                </header>
                                <p>Veja que casinha legal!</p>
                            </div>
                        </section>

                    </div>
                    <div class="col-4 col-12-medium">

                        <!-- Box -->
                        <section class="box feature">
                            <a href="#" class="image featured"><img src="images/pic03.jpg" alt="" /></a>
                            <div class="inner">
                                <header>
                                    <h2>Imóvel N#3</h2>
                                    <p>Linda casa "Branca"</p>
                                </header>
                                <p>Veja que casinha legal!</p>
                            </div>
                        </section>

                    </div>
                </div>
            </div>
        </div>

        <!-- Main -->
        <div id="main-wrapper">
            <div class="container">
                <div class="row gtr-200">
                    <div class="col-4 col-12-medium">

                        <!-- Sidebar -->
                        <div id="sidebar">
                            <section class="widget thumbnails">
                                <h3>Você pode se interessar... rsrs</h3>
                                <div class="grid">
                                    <div class="row gtr-50">
                                        <div class="col-6"><a href="#" class="image fit"><img src="images/pic04.jpg" alt="" /></a></div>
                                        <div class="col-6"><a href="#" class="image fit"><img src="images/pic05.jpg" alt="" /></a></div>
                                        <div class="col-6"><a href="#" class="image fit"><img src="images/pic06.jpg" alt="" /></a></div>
                                        <div class="col-6"><a href="#" class="image fit"><img src="images/pic07.jpg" alt="" /></a></div>
                                    </div>
                                </div>
                                <a href="#" class="button icon fa-file-alt">Veja Mais</a>
                            </section>
                        </div>

                    </div>
                    <div class="col-8 col-12-medium imp-medium">

                        <!-- Content -->
                        <div id="content">
                            <section class="last">
                                <h2>Então, quem sou eu?</h2>
                                <p>
                                    Me chamo <strong>Gustavo Henrique Lopes Martins</strong>, Sou casado, tenho 29 anos, moro com esposa e filho <a href="https://www.linkedin.com/in/gustavo-henrique-lopes-martins-361789192/">Me conheça!</a>.
                                    Amo tecnologia, e tudo quanto é novidade <a href="https://github.com/Gustavo-H-Martins">Meu GitHub</a>, então, estou a disposição para feedback e orientação quanto a melhorias.
                                </p>
                                <p>Veja mais no link de redirec.</p>
                                <a href="https://github.com/Gustavo-H-Martins" class="button icon solid fa-arrow-circle-right">continue lendo</a>
                            </section>
                        </div>

                    </div>
                </div>
            </div>
        </div>

        <!-- Footer -->
        <div id="footer-wrapper">
            <footer id="footer" class="container">
                <div class="row">


                    <div class="col-3 col-6-medium col-12-small">

                        <!-- Contact -->
                        <section class="widget contact last">
                            <h3>Minhas Redes</h3>
                            <ul>

                                <li><a href="https://www.facebook.com/ghustavol" class="icon brands fa-facebook-f"><span class="label">Facebook</span></a></li>
                                <li><a href="https://www.instagram.com/gustavojoana10" class="icon brands fa-instagram"><span class="label">Instagram</span></a></li>
                            </ul>
                            <p>
                                1234 rua ficticia<br />
                                aquela cidade, N# 00000<br />
                                (0800) 555-1234
                            </p>
                        </section>

                    </div>
                </div>
                <div class="row">
                    <div class="col-12">
                        <div id="copyright">
                            <ul class="menu">
                                <li>&copy; Untitled. All rights reserved</li>
                                <li>Design: <a href="https://github.com/Gustavo-H-Martins">Gustavo H. Lopes</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </footer>
        </div>

    </div>

    <!-- Scripts -->

    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/jquery.dropotron.min.js"></script>
    <script src="~/js/browser.min.js"></script>
    <script src="~/js/breakpoints.min.js"></script>
    <script src="~/js/util.js"></script>
    <script src="~/js/main.js"></script>
    <!--Eu terminando a baianagem do teste-->
    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - Corretora_imovel_gustavo_martins - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
